// BOJ_2754 : 학점계산
// https://www.acmicpc.net/problem/2754
// 20240514T1238 - 20240514T1245 
#include <iostream>
int main()
{
	std::string s;
	std::cin >> s;
	if (s == "A+") std::cout << "4.3" << "\n";
	else if (s == "A0") std::cout << "4.0" << "\n";
	else if (s == "A-") std::cout << "3.7" << "\n";

	else if (s == "B+") std::cout << "3.3" << "\n";
	else if (s == "B0") std::cout << "3.0" << "\n";
	else if (s == "B-") std::cout << "2.7" << "\n";

	else if (s == "C+") std::cout << "2.3" << "\n";
	else if (s == "C0") std::cout << "2.0" << "\n";
	else if (s == "C-") std::cout << "1.7" << "\n";

	else if (s == "D+") std::cout << "1.3" << "\n";
	else if (s == "D0") std::cout << "1.0" << "\n";
	else if (s == "D-") std::cout << "0.7" << "\n";

	else if (s == "F") std::cout << "0.0" << "\n";
}
